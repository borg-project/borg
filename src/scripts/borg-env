#!/usr/bin/env python2.6
"""
@author: Bryan Silverthorn <bcs@cargo-cult.org>
"""

if __name__ == "__main__":
    from os.path import join
    from borg    import (
        execute_borg,
        get_default_root,
        )

    execute_borg(
        root      = join(get_default_root(), ".."),
        support   = "etc",
        submodule = "dep",
        )

