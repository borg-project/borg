def build(context):
    context(
        source = [
            "__init__.py",
            "get_calibration.py",
            "get_features.py",
            "run_solvers.py",
            "solve.py",
            "train.py",
            "plan.py",
            "simulate.py",
            "bundle_run_data.py",
            "ground.py",
            "reextend.py",
            ],
        install_path = "${PYTHONDIR}/borg/tools",
        )

